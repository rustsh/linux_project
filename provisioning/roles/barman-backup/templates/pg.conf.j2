[pg]
description = "Our main PostgreSQL server"
conninfo = host={{ master_host_ip }} user={{ barman_login }} dbname=postgres
streaming_conninfo = host={{ master_host_ip }} user={{ streaming_barman_login }} dbname=postgres
backup_method = postgres
streaming_archiver = on
slot_name = barman
create_slot = auto
path_prefix = "/usr/pgsql-11/bin"
